@model MyCVProject.Models.Entity.SosyalMedya
@{
    ViewBag.Title = "SosyalMedyaEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Yeni Sosyal Medya Ekle</h2>
<br />
<form method="post">
    @Html.Label("Sosyal Medya Adı: ")
    @Html.TextBoxFor(a => a.Ad, new { @class = "form-control" })
    <br />
    @Html.Label("Sosyal Medya Linki: ")
    @Html.TextBoxFor(a => a.Link, new { @class = "form-control" })
    <br />
    @Html.Label("Sosyal Medya İkonu: ")
    @Html.TextBoxFor(a => a.Ikon, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Yeni Sosyal Medya Ekle</button>
</form>



