@model MyCVProject.Models.Entity.Admin

@{
    ViewBag.Title = "AdminGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Admin Kullanıcısını Güncelleme</h4>
<br />
<form method="post">
    @Html.Label("Kullanıcı Adı: ")
    @Html.TextBoxFor(n => n.KullaniciAdi, new { @class = "form-control" })
    <br />
    @Html.Label("Parola")
    @Html.TextBoxFor(n => n.Parola, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Admin Güncelle</button>
</form>

