@model MyCVProject.Models.Entity.Deneyimlerim
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Yeni Deneyim Ekleme</h4>
<br />
<form method="post">

    <input type="text" name="Baslik" class="form-control" placeholder="Başlığı Yazınız" />
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt detayı giriniz" />
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Çalışma tarih aralığını giriniz" />
    <br />
    <input type="text" name="DeneyimLink" class="form-control" placeholder="Deneyim Bağlantısı Ekle" />
    <br />
    <textarea placeholder="Açıklamanızı giriniz" name="Aciklama" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Yeni Deneyimi Kaydet</button>
</form>
