@model MyCVProject.Models.Entity.Deneyimlerim
@{
    ViewBag.Title = "DeneyimGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Deneyim Güncelleme</h4>
<br />
<form method="post">
    <br />
    @Html.Label("Başlık")
    @Html.TextBoxFor(x => x.Baslik, new { @class = "form-control"})
    <br />
    @Html.Label("Alt Başlık")
    @Html.TextBoxFor(x => x.AltBaslik, new { @class = "form-control"})
    <br />
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control"})
    <br />
    @Html.Label("Deneyim Bağlantısı")
    @Html.TextBoxFor(x=>x.DeneyimLink, new {@class = "form-control"})
    <br />
    @Html.Label("Açıklama")
    @Html.TextAreaFor(x => x.Aciklama, new { @class = "form-control", @rows = 3 })
    <br />
    <button class="btn btn-info">Deneyimi Güncelle</button>
</form>
